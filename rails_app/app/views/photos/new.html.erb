<%= form_with(model: @photo, url: photos_path, local: true, method: :post, class: '') do |f| %>
  <div>
    <p><%= f.label :title %></p>
    <p><%= f.text_field :title, placeholder: 'title', requried: true %></p>
  </div>
  <div>
    <p><%= f.label :image %></p>
    <p><%= f.file_field :image, required: true %></p>
  </div>
  <%= f.submit 'アップロード' %>

<% end %>
