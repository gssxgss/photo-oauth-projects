<div class="text-center">
  <h1>ログイン</h1>

  <%= form_with url: login_path do |f| %>

    <!-- Field of sessoin form -->
    <div class='row'>
      <%= f.label :user_id %>
      <%= f.text_field :user_id, placeholder: 'user id' %>
    </div>

    <div class='row'>
      <%= f.label :password %>
      <%= f.password_field :password, placeholder: 'password' %>
    </div>

    <div class='row'>
      <%= f.submit 'ログイン' %>
    </div>
  <% end %>
</div>
